    								<w2:item>
    									<w2:label><![CDATA[green]]></w2:label>
    									<w2:value><![CDATA[]]></w2:value>
    								</w2:item>
    								<w2:item>
    									<w2:label><![CDATA[blue]]></w2:label>
    									<w2:value><![CDATA[]]></w2:value>
