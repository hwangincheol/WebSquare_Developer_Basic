		<script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function () {
    // Fusionchart 'cht_exam1'의 초기 속성을 설정합니다.
    // Scroll Single Series Charts
    cht_exam1.setChartAttribute({
        "caption": rad_chartType1.getText(), // radio에 선택된 text를 차트의 caption으로 지정.
        "yAxisMaxValue": "32000",
        "canvasTopPadding": 6,
        "canvasRightMargin": 10,
        "canvasLeftPadding": 6,
        "canvasRightPadding": 6,
        "chartBottomMargin": 16,
        "scrollheight": "12",
        "flatScrollBars": "1",
        "scrollShowButtons": "1",
        "scrollColor": "#eeeeee"
    });

    // 지정한 속성을 바탕으로 브라우저에 그립니다.
    cht_exam1.draw();

    // Fusionchart 'cht_exam2'의 초기 속성을 설정합니다.
    // Scroll Stacked Charts
    cht_exam2.setChartAttribute({
        "caption": rad_chartType2.getText(), // radio에 선택된 text를 차트의 caption으로 지정.
        "canvasTopPadding": 6,
        "canvasRightMargin": 10,
        "canvasLeftPadding": 6,
        "canvasRightPadding": 6,
        "chartBottomMargin": 16,
        "scrollheight": "12",
        "flatScrollBars": "1",
        "scrollShowButtons": "1",
        "scrollColor": "#eeeeee"
    });

    // 지정한 속성을 바탕으로 브라우저에 그립니다.
    cht_exam2.draw();
};

/**
 * 차트 'Scroll Single Series Charts' 타입이 구성된 Radio 'rad_chartType1'의 이벤트 'onviewchange' 핸들러
 */
scwin.rad_chartType1_onviewchange = function (info) {
    let chartAlias = info.value;
    // Fusionchart 'cht_exam1'의 속성을 설정합니다.
    // radio에 선택된 text를 차트의 caption으로 지정.
    cht_exam1.setChartAttribute({
        "caption": this.getText()
    });

    cht_exam1.changeType(chartAlias);

    // 'Scroll Single Series Charts' 타입 변경 예시)
    // Fucionchart의 속성 'seriesColumns' 예시) [['zone1']]

    // Fusionchart 'cht_exam1'의 차트 타입을 'Scroll Column 2D'로 변경합니다.
    // cht_exam1.changeType('scrollcolumn2d');

    // Fusionchart 'cht_exam1'의 차트 타입을 'Scroll Bar 2D'로 변경합니다.
    // cht_exam1.changeType('scrollbar2d');

    // Fusionchart 'cht_exam1'의 차트 타입을 'Scroll Line 2D'로 변경합니다.
    // cht_exam1.changeType('scrollline2d');
    
    // Fusionchart 'cht_exam1'의 차트 타입을 'Scroll Area 2D'로 변경합니다.
    // cht_exam1.changeType('scrollarea2d');
};

/**
 * 차트 'Scroll Stacked Charts' 타입이 구성된 Radio 'rad_chartType2'의 이벤트 'onviewchange' 핸들러
 */
scwin.rad_chartType2_onviewchange = function (info) {
    let chartAlias = info.value;
    // Fusionchart 'cht_exam2'의 속성을 설정합니다.
    // radio에 선택된 text를 차트의 caption으로 지정.
    cht_exam2.setChartAttribute({
        "caption": this.getText()
    });

    cht_exam2.changeType(chartAlias);

    // 'Scroll Stacked Charts' 타입 변경 예시)
    // Fucionchart의 속성 'seriesColumns' 예시) [['zone1','zone2','zone3']]

    // Fusionchart 'cht_exam2'의 차트 타입을 'Scroll Stacked Column 2D'로 변경합니다.
    // cht_exam2.changeType('scrollstackedcolumn2d');

    // Fusionchart 'cht_exam2'의 차트 타입을 'Scroll Stacked Bar 2D'로 변경합니다.
    // cht_exam2.changeType('scrollstackedbar2d');
};
]]></script>
