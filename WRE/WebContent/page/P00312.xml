		<script lazy="false" type="text/javascript"><![CDATA[
scwin.onpageload = function () {
    // 데이터 정렬
    dlt_chartType.multisort({
        "sortIndex": "order",
        "sortOrder": "1"
    });

    // UI 생성
    scwin.dispGenerator();
};

scwin.dispGenerator = function () {
    let arrData = dlt_chartType.getAllJSON();
    let _cmpGenCatetory = gen_category;
    let _cmpGen;
    let numCateIndex;
    let numGenIndex;
    let preCategory;
    let strTbxValue = "";

    for (let i = 0, i_max = arrData.length; i < i_max; i++) {
        let item = arrData[i];

        if (preCategory != item.category) {
            numCateIndex = _cmpGenCatetory.getLength();
            _cmpGenCatetory.insertChild();
            _cmpGenCatetory.getChild(numCateIndex, "tbx_category").setValue(item.category);
            _cmpGen = _cmpGenCatetory.getChild(numCateIndex, "gen_exam1");
            preCategory = item.category;

            _cmpGen.insertChild();
            _cmpGen.getChild(0, "tbx_type").setValue("Chart Type");
            _cmpGen.getChild(0, "tbx_alias").setValue("Chart Alias");

            numGenIndex = 1;

            strTbxValue += "// " + item.category + "\n";
        }
        
        _cmpGen.insertChild();
        _cmpGen.getChild(numGenIndex, "tbx_type").setValue(item.type);
        _cmpGen.getChild(numGenIndex, "tbx_alias").setValue(item.alias);

        strTbxValue += item.type + " : " + item.alias + "\n";

        numGenIndex++;
    };

    txa_result.setValue(strTbxValue);
};

]]></script>
