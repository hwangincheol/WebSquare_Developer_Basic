										<xf:value><![CDATA[]]>
										</xf:value>
									</xf:item>
									<xf:item>
										<xf:label><![CDATA[Lable 4]]></xf:label>
										<xf:value><![CDATA[]]>
										</xf:value>
									</xf:item>
								</xf:choices>
							</xf:select1>
						</xf:group>
						<xf:group id="" style="margin-bottom: 5px;">
							<w2:span id="" label="all :" style="width :80px;text-align: right;margin-right: 10px;"></w2:span>
							<xf:select1 allOption="true" appearance="minimal" chooseOption="true" direction="auto"
								disabled="false" id="sbx_main2" ref="" style="width: 148px;margin-right: 15px;"
								submenuSize="auto" textAlign="left" optionOrder="all">
								<xf:choices>
