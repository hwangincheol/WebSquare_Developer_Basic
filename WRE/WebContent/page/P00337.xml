						<w2:textbox class="ws5_example_txt_normal" escape="false" id=""
							label="SelectBox의 기본 상태와 선택 항목을 열었을 때 optionOrder에 따라 표시되는 순서를 확인합니다." style="">
						</w2:textbox>
						<xf:group id="" style="margin-bottom: 5px;">
							<w2:span id="" label="choose : " style="width :80px;text-align: right;margin-right: 10px;"></w2:span>
							<xf:select1 id="sbx_main1" chooseOption="true" style="width: 148px;margin-right: 15px;" submenuSize="auto"
								allOption="true" disabled="false" direction="auto" appearance="minimal" chooseOptionLabel="" ref="" textAlign="left"
								optionOrder="choose">
								<xf:choices>
									<xf:item>
										<xf:label><![CDATA[Label 1]]></xf:label>
										<xf:value><![CDATA[]]>
