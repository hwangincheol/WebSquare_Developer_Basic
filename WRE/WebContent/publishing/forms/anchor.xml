			.btn.point.w2anchor_disabled {border: 1px solid #d2d4d9; background:#d2d4d9; color:#edeef3;}
			.btn.basic { border: 1px solid #757984; color:#757984; background:#fff;font-weight: bold;}
			.btn.basic:hover {background:#757984; color:#FFF;}
			.btn.basic.w2anchor_disabled {border: 1px solid #d2d4d9; background:#d2d4d9; color:#edeef3;}
