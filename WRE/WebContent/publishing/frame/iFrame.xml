    						<w2:attributes>
    							<w2:scope>row</w2:scope>
    						</w2:attributes>
    						<w2:textbox class="" id="" label="spa" style="" tagname="p"></w2:textbox>
    					</xf:group>
    					<xf:group tagname="td" class="w2tb_td">
    						<w2:attributes></w2:attributes>
    						<w2:textbox class="" id=""
    							label="SPA 모드 적용 여부.&lt;br/&gt;src 화면이 웹스퀘어 화면인 경우만 해당.&lt;br/&gt;SPA 모드는 IFrame 내의 웹스퀘어 자원들을 재사용.&lt;br/&gt;src 화면이 동적으로 변경될 경우 해당 화면의 소스와 필수 리소스만 로딩하여 적용."
    							style="" tagname="p">
    						</w2:textbox>
    						<xf:group class="grp_option" id="" style="">
    							<w2:span class="badge" id="" label="true" style=""></w2:span>
    							<w2:span class="badge default" id="" label="false" style=""></w2:span>
    						</xf:group>
    					</xf:group>
    				</xf:group>
    				<xf:group tagname="tr">
    					<xf:group tagname="th" class="w2tb_th">
