			<xf:group class="msgbox" id="" style="border:1px dashed #ccc;">
				<xf:checkcombobox allOption="" appearance="minimal" chooseOption="" class="wq_ccb" direction="auto" disabled="false"
					displayAllOptionLabel="false" displayMode="label" id="" style='width:200px;margin-right:26px;' submenuSize="auto">
