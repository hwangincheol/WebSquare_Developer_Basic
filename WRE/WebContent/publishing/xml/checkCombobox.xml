							<xf:value></xf:value>
						</xf:item>
						<xf:item>
							<xf:label><![CDATA[Green]]></xf:label>
							<xf:value></xf:value>
						</xf:item>
					</xf:choices>
				</xf:checkcombobox>
				<xf:checkcombobox allOption="" appearance="minimal" chooseOption="" class="wq_ccb" direction="auto" disabled="true"
					displayAllOptionLabel="false" displayMode="label" id="" style="width:200px;" submenuSize="auto">
					<xf:choices>
						<xf:item>
							<xf:label><![CDATA[Red]]></xf:label>
							<xf:value></xf:value>
						</xf:item>
						<xf:item>
							<xf:label><![CDATA[Blue]]></xf:label>
							<xf:value></xf:value>
						</xf:item>
